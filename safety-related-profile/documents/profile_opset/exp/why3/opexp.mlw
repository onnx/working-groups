(**
    Specification of Exp operation on tensors.
 *)
module Exp
  use int.Int
  use map.Map
  use utils.Same
  use tensor.Shape
  use tensor.Tensor
  use real.Real
  use real.Exp
  let function exp (a : tensor real) : tensor real =
    ensures { forall i. result.value[i] = exp a.value[i] }
  {
    shape = a.shape ;
    value = fun i -> exp a.value[i] ;
  }
end
(**
    Specification of Log operation on tensors.
 *)
module Log
  use int.Int
  use map.Map
  use utils.Same
  use tensor.Shape
  use tensor.Tensor
  use real.Real
  use real.Log
  let function log (a : tensor real) : tensor real =
    ensures { forall i. result.value[i] = log a.value[i] }
  {
    shape = a.shape ;
    value = fun i -> log a.value[i] ;
  }
end
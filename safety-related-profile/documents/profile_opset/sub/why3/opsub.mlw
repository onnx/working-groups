(**
    Specification of Sub operation on tensors.
 *)

module Sub
  use int.Int
  use map.Map
  use utils.Same
  use tensor.Shape
  use tensor.Tensor

  let function sub (a b : tensor 'a) : tensor 'a =
  {
    shape = same a.shape b.shape ;
    value = fun i -> a.value[i] - b.value[i] ;
  }

end